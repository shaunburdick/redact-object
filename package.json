  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash.isplainobject": "^4.0.9",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-shaunburdick": "^2.0.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "homepage": "https://github.com/shaunburdick/redact-object",
